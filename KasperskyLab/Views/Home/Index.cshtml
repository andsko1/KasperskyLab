@{
	ViewBag.Title = "тестовое задание";
}
<style>
	button[type=submit] {
		margin-top: 20px;
	}

	.field-validation-error {
		color: red;
	}

	.validation-summary-errors {
		color: Red;
	}
	.panel-body {
		max-height: 200px;
		overflow-y: scroll;
	}
</style>
<div id="genModal" class="modal fade" role="dialog">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title">Генерирование</h4>
			</div>
			<div id="genModalBody">
				<div col-sm-12 col-md-12 col-lg-12 col-xl-12>
					<div class="form-group">
						Минимальное значение
						<input class="form-control" id="gen-min" value="">
					</div>
					<div class="form-group">
						Максимальное значение
						<input class="form-control" id="gen-max">
					</div>
					<div class="form-group">
						Знаков после запятой
						<input class="form-control" id="gen-precision">
					</div>
					<div class="form-group">
						Колличество
						<input class="form-control" id="gen-count">
					</div>
				</div>
			</div>
			<div class="modal-footer">
				<div class="btn btn-success" data-dismiss="modal" id="gen-confirm">Сгенерировать</div>
			</div>
		</div>
	</div>
</div> 
<div class="row">
	<ul class="nav nav-tabs">
		<li class="active"><a data-toggle="tab" href="#parallel-queue">Задание 1</a></li>
		<li><a data-toggle="tab" href="#stack-search">Задание 2</a></li>
	</ul>
	<div class="tab-content">
		<div class="tab-pane fade active in" id="parallel-queue">
			@{Html.RenderAction("ParallelQueue", "Home");}
		</div>
		<div class="tab-pane fade" id="stack-search">
			@{Html.RenderAction("StackSearch", "Home");}
		</div>
	</div>
</div>